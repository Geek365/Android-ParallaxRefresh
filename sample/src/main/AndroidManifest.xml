<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="net.soulwolf.widget.parallaxrefresh.sample" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="net.soulwolf.widget.parallaxrefresh.sample.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="net.soulwolf.widget.parallaxrefresh.sample.ParallaxScrollViewActivity"
            android:label="@string/title_activity_parallax_scroll_view" >
        </activity>
        <activity
            android:name="net.soulwolf.widget.parallaxrefresh.sample.ParallaxListViewActivity"
            android:label="@string/title_activity_parallax_list_view" >
        </activity>
    </application>

</manifest>
